{
    "name": "Ooyala API",
    "apiVersion": "v2",
    "description": "Ooyala is an API that allows you to interact with the Ooyala REST services.",
    "operations": {
        "GetAssets": {
            "httpMethod": "GET",
            "uri": "assets{?include}",
            "description": "List Assets",
            "parameters": {
                "where": {
                    "location": "query",
                    "type": "string"
                },
                "include": {
                    "location": "query",
                    "type": "string",
                    "pattern": "/^(metadata,labels|labels,metadata|metadata|labels)$/"
                },
                "limit": {
                    "location": "query",
                    "type": "integer"
                }
            }
        }
    }
}